<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lol.chemists">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".lol" />
        <activity
            android:name=".PatientDetails"
            android:label="@string/title_activity_patient_details"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PrescriptionList"
            android:label="@string/title_activity_prescription_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
